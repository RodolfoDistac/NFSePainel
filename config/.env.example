# =====================================================================
# NFSePainel – Exemplo de variáveis de ambiente (.env.example)
# Copie para ".env" e preencha os valores sensíveis.
# NÃO versionar o arquivo .env.
# =====================================================================

# ===== Domínio (SQL Anywhere / Sybase) =====
# Se tiver DSN configurado no ODBC do Windows, preencha aqui e deixe os demais campos.
SYBASE_DSN=
SYBASE_DRIVER=SQL Anywhere 17
SYBASE_HOST=IGOR
SYBASE_PORT=2638
SYBASE_DB=Contabil
SYBASE_UID=ESTACAO08
SYBASE_PWD=

# ===== Oracle – perfis usados pelo app (CadastroDistac / Importação Baixa) =====
# CadastroDistac (servidor)
ORACLE_CAD_HOST=192.168.236.44
ORACLE_CAD_PORT=1521
ORACLE_CAD_SERVICE=XE
ORACLE_CAD_USER=CADASTRODISTAC
ORACLE_CAD_PASSWORD=DISTAC

# Importação Baixa (servidor)
ORACLE_BAIXA_HOST=192.168.236.44
ORACLE_BAIXA_PORT=1521
ORACLE_BAIXA_SERVICE=XE
ORACLE_BAIXA_USER=IMPORTACAO_BAIXA_PADRAO
ORACLE_BAIXA_PASSWORD=DISTAC

# Query para listar empresas no "Login Empresa" (ajuste conforme seu esquema)
ORACLE_CAD_EMPRESAS_QUERY=SELECT ID, NOME, CNPJ FROM EMPRESAS WHERE ATIVA=1 ORDER BY NOME

# ===== Oracle – modo do driver =====
# 'thin' (padrão) ou 'thick' (requer Oracle Instant Client instalado)
ORACLE_MODE=thin
# Se usar thick, aponte para a pasta do Instant Client, ex.:
# ORACLE_CLIENT_LIB_DIR=C:/oracle/instantclient_19_24
ORACLE_CLIENT_LIB_DIR=C:/app/Rodolfo/product/instantclient_21_19
# ===== App =====
EXPORT_DIR=C:/A
APP_THEME=SystemDefault

# Cache de empresas (segundos)
EMPRESAS_TTL=60
